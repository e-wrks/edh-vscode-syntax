{
    "publisher": "ComplYue",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/e-wrks/edh-vscode-syntax"
    },
    "name": "edh-vscode-syntax",
    "displayName": "Đ (Edh) Syntax Highlighting",
    "description": "Syntax Highlighting for Đ (Edh)",
    "version": "1.8.1",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "edh",
                "aliases": [
                    "Edh",
                    "edh"
                ],
                "extensions": [
                    ".edh"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "edh",
                "scopeName": "source.edh",
                "path": "./syntaxes/edh.tmLanguage.json"
            }
        ],
        "themes": [
            {
                "id": "Edh - Dark+",
                "label": "Đ (Edh) - Dark+",
                "uiTheme": "vs-dark",
                "path": "./themes/edh-dark_plus.json"
            }
        ]
    }
}
