{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"include": "./CalmWarm.json",
	"tokenColors": [
		{
			"name": "Concurrency Control",
			"scope": "keyword.concur.control",
			"settings": {
				"foreground": "#d3d630"
			}
		},
		{
			"name": "Keyword Control",
			"scope": [
				"keyword.control.edh"
			],
			"settings": {
				"foreground": "#c393cf"
			}
		},
		{
			"name": "Storage Type",
			"scope": "storage.type.edh",
			"settings": {
				"foreground": "#b9b9b9"
			}
		},
		{
			"name": "Storage Modifier",
			"scope": "storage.modifier.edh",
			"settings": {
				"foreground": "#dadada"
			}
		},
		{
			"name": "Support Module",
			"scope": "support.module.edh",
			"settings": {
				"foreground": "#c76267"
			}
		},
		{
			"name": "Support Function",
			"scope": "support.function.edh",
			"settings": {
				"foreground": "#dd676d"
			}
		},
		{
			"name": "String",
			"scope": "string.edh",
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"name": "Decimal",
			"scope": "constant.numeric.decimal.edh",
			"settings": {
				"foreground": "#5799df"
			}
		},
		{
			"name": "Line Comment",
			"scope": "comment.line.edh",
			"settings": {
				"foreground": "#6e776a"
			}
		},
		{
			"name": "Block Comment",
			"scope": "comment.block.edh",
			"settings": {
				"foreground": "#6e776a"
			}
		},
		{
			"name": "Expr Keyword",
			"scope": "keyword.other.expr.edh",
			"settings": {
				"foreground": "#d3d475"
			}
		},
		{
			"name": "IncExpr Keyword",
			"scope": "keyword.other.iexpr.edh",
			"settings": {
				"foreground": "#d3d475"
			}
		},
		{
			"name": "Sink Keyword",
			"scope": "keyword.other.sink.edh",
			"settings": {
				"foreground": "#d3d630"
			}
		},
		{
			"name": "Control in Expression",
			"scope": "expr.keyword.control.edh",
			"settings": {
				"foreground": "#64bacf"
			}
		},
		{
			"name": "Index Operator in Expression",
			"scope": "expr.keyword.operator.index.edh",
			"settings": {
				"foreground": "#7eca3f"
			}
		},
		{
			"name": "Alphanumeric Operator in Expression",
			"scope": "expr.keyword.operator.alphanumeric.edh",
			"settings": {
				"foreground": "#7eca3f"
			}
		},
		{
			"name": "Operator in Expression",
			"scope": "expr.keyword.operator.edh",
			"settings": {
				"foreground": "#ca7e3f"
			}
		},
		{
			"name": "Alphanumeric Variable",
			"scope": "variable.other.alphanumeric.edh",
			"settings": {
				"foreground": "#6fa191"
			}
		},
		{
			"name": "Symbolic Variable",
			"scope": "variable.other.symbolic.edh",
			"settings": {
				"foreground": "#7f9e5c"
			}
		},
		{
			"name": "Intrinsic Constant",
			"scope": "constant.language.edh",
			"settings": {
				"foreground": "#5a5ab4"
			}
		},
		{
			"name": "Intrinsic Variable",
			"scope": "variable.language.edh",
			"settings": {
				"foreground": "#a5464b"
			}
		}
	],
	"semanticHighlighting": true
}
