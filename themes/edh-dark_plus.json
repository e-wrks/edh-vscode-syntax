{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"include": "./dark_plus.json",
	"tokenColors": [
		{
			"name": "Concurrency Control",
			"scope": "keyword.concur.control",
			"settings": {
				"foreground": "#d3d630"
			}
		},
		{
			"name": "Keyword Control",
			"scope": [
				"keyword.control.edh"
			],
			"settings": {
				"foreground": "#c393cf"
			}
		},
		{
			"name": "Storage Type",
			"scope": "storage.type.edh",
			"settings": {
				"foreground": "#b9b9b9"
			}
		},
		{
			"name": "Storage Modifier",
			"scope": "storage.modifier.edh",
			"settings": {
				"foreground": "#dadada"
			}
		},
		{
			"name": "Support Module",
			"scope": "support.module.edh",
			"settings": {
				"foreground": "#c76267"
			}
		},
		{
			"name": "Support Function",
			"scope": "support.function.edh",
			"settings": {
				"foreground": "#dd676d"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"fontStyle": "",
				"foreground": "#98d449"
			}
		},
		{
			"name": "Comment",
			"scope": "comment.edh",
			"settings": {
				"fontStyle": "",
				"foreground": "#b6dbb4"
			}
		},
		{
			"name": "Expr Keyword",
			"scope": "keyword.other.expr.edh",
			"settings": {
				"foreground": "#eb7601"
			}
		},
		{
			"name": "Sink Keyword",
			"scope": "keyword.other.sink.edh",
			"settings": {
				"foreground": "#e77400"
			}
		},
		{
			"name": "Control in Expression",
			"scope": "expr.keyword.control.edh",
			"settings": {
				"foreground": "#64bacf"
			}
		},
		{
			"name": "Operator in Expression",
			"scope": "expr.keyword.operator.edh",
			"settings": {
				"foreground": "#d6945d"
			}
		},
		{
			"name": "Alphanumeric Variable",
			"scope": "variable.other.alphanumeric.edh",
			"settings": {
				"foreground": "#99e0c9"
			}
		},
		{
			"name": "Symbolic Variable",
			"scope": "variable.other.symbolic.edh",
			"settings": {
				"foreground": "#75a7dd"
			}
		},
		{
			"name": "Intrinsic Constant",
			"scope": "constant.language.edh",
			"settings": {
				"foreground": "#7373eb"
			}
		},
		{
			"name": "Intrinsic Variable",
			"scope": "variable.language.edh",
			"settings": {
				"foreground": "#6e69dd"
			}
		}
	],
	"semanticHighlighting": true
}