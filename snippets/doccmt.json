{
  "Doc Comment": {
    "prefix": "{",
    "body": [
      "{## ${1:Summary}",
      "${0:Details}",
      " #"
    ],
    "description": "a doc comment"
  },
  "Doc Block": {
    "prefix": "{;",
    "body": [
      "{## ${1:Title}",
      " # - ${2:Summary}",
      " #} {;",
      "  $0",
      ""
    ],
    "description": "a doc block"
  },
  "Doc Block w/ Bookmark": {
    "prefix": "{;b",
    "body": [
      "{## ${1:Title}",
      " # - ${2:Summary}",
      "",
      "${4:Details}",
      "",
      " #} {;",
      "  {## ${1:Title} #}",
      "  export ${3:bookmark'identifier} = bookmark()",
      "",
      "  $0",
      ""
    ],
    "description": "a doc block with bookmark"
  },
  "Block Comment": {
    "prefix": "{#",
    "body": [
      "{# $1",
      "$0",
      " #"
    ],
    "description": "a block comment"
  },
  "Code Cell": {
    "prefix": "#%",
    "body": [
      "# %% # ${1:Run this cell to }",
      "$0",
      "# %# # ${2:See? The code above }"
    ],
    "description": "a code cell"
  },
  "Code Block": {
    "prefix": "#{",
    "body": [
      "# %{ # ${1:Run this block to }",
      "$0",
      "# %% # ${2:Run this cell to }",
      "",
      "# %% # ${3:Run this cell to }",
      "",
      "# %} # End * $1"
    ],
    "description": "a code cell"
  },
  "Bookmark": {
    "prefix": "bookmark",
    "body": [
      "{## ${1:Bookmark Label} #}",
      "export ${0:bookmark'identifier} = bookmark()"
    ],
    "description": "a bookmark"
  },
  "Đ the letter": {
    "prefix": "Edh",
    "body": "Đ",
    "description": "Đ the letter"
  }
}